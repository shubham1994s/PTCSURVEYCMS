                <td>@Html.DisplayFor(model => model.HFSNo, new { @class = "p-5", style = "font-family:Arial, FontAwesome" })</td>
                <td>@Html.DisplayFor(model => model.Legal, new { @class = "p-5", style = "font-family:Arial, FontAwesome" })</td>

