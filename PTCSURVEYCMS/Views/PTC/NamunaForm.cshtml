<script type="text/javascript">
    $(document).ready(function () {
        var maxField = 10; //Input fields increment limitation
        var addButton = $('.add_button'); //Add button selector
        var wrapper = $('.field_wrapper'); //Input field wrapper
        //var fieldHTML = '<div><input type="text" name="field_name[]" value=""/><a href="javascript:void(0);" class="remove_button"><img src="remove-icon.png"/></a></div>'; //New input field html
        var fieldHTML = '<div id="demo"><div class="d-flex flex-row"> <div class="form-group pr-4"> <label class="label"> मालमत्ताधारकाचे नाव <span id="ownercount"></span> /<br /> Prop. Owner Name </label> </div><div class="form-group pr-4"><label class="label">प्रथम नाव/ First Name</label><input type = "text" class="form-control" style = "font-family:Arial, FontAwesome" name ="PropOwnerFirstName[]"/><span class="text-danger" id="err_empidd"></span></div><div class="form-group pr-4"><label class="label">मधले नाव / Middle Name</label><input type = "text" class="form-control" style = "font-family:Arial, FontAwesome" name ="PropOwnerMiddleName[]"/><span class="text-danger" id="err_empidd"></span><span class="glyphicon form-control-feedback" aria-hidden="true"></span><span class="help-block with-errors"></span></div> <div class="form-group pr-4"><label class="label">आडनाव / Last Name</label><input type = "text" class="form-control" style = "font-family:Arial, FontAwesome" name = "PropOwnerLastName[]" /><input type="hidden" /><span class="text-danger" id="err_empidd"></span><span class="glyphicon form-control-feedback" aria-hidden="true"></span> <span class="help-block with-errors"></span></div><a href="javascript:void(0);" class="remove_button"><div class="Ncircle"></div></a></div></div>'; //New input field html
        var x = 1; //Initial field counter is 1

        //Once add button is clicked
        $(addButton).click(function () {
            //Check maximum number of input fields
            if (x < maxField) {
                x++; //Increment field counter
                $(wrapper).append(fieldHTML); //Add field html
            }
        });

        //Once remove button is clicked
        $(wrapper).on('click', '.remove_button', function (e) {
            e.preventDefault();
            $(this).parent('div').remove(); //Remove field html
            x--; //Decrement field counter
        });
    });
</script>


