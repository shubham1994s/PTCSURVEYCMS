@model BLL.ViewModel.PropertyMasterVM
@{
    ViewBag.Title = "SurveyForm";
    Layout = "";
}
<style>
    table, th, td {
        border: 1px solid black;
        border-collapse: collapse;
    }

    .p_5 {
        padding: 5px 5px 5px 5px !important;
    }

    .border1 {
